        <Label Content="Длина" Margin="0,30,208,504" FontSize="15"/>
        <Label Content="Ширна" Margin="0,88,194,452" FontSize="15"/>
        <Label Content="Помецение " Margin="0,0,129,546" FontSize="15"/>
        <TextBox x:Name="TBlengthR" TextWrapping="Wrap" Width="120" Margin="204,30,14,504" TextChanged="TextBox_TextChanged"/>
        <TextBox x:Name="TBwidthR" TextWrapping="Wrap" Width="120" Margin="204,88,14,452"/>
        <Grid Margin="0,572,-1005,-146">
            <Button x:Name="BAddRoom" Content="Button" HorizontalAlignment="Left" Margin="128,30,0,0" VerticalAlignment="Top" Height="38" Width="132"/>
            <Button x:Name="BAddOffice" Content="Button" Margin="756,35,429,73"/>
