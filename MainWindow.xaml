            <Label Content="ширина" Margin="0,109,189,440" FontSize="15"/>
            <Label Content="длина" Margin="0,50,189,496" FontSize="15"/>
            <Label Content="жилое помещение" Margin="0,0,149,536" FontSize="15"/>
            <Label Content="число окон" Margin="0,161,189,390" FontSize="15"/>
            <TextBox x:Name="TBlengthL" TextWrapping="Wrap" Width="120" Margin="204,30,14,504" TextChanged="TextBox_TextChanged"/>
